  "license": "ISC",
  "dependencies":{
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "fis-parser-es6-babel": "^1.0.0",
    "fis-parser-less": "^0.1.3",
    "fis3": "^3.4.22",
    "fis3-deploy-zip": "^2.0.0",
    "fis3-hook-commonjs": "^0.1.15",
    "fis3-hook-module": "^1.1.1",
    "fis3-hook-relative": "^1.0.7",
    "fis3-postpackager-loader": "^1.3.17",
    "fis3-preprocessor-js-require-css": "*",
    "vinyl-fs": "^2.4.3",
    "win-spawn": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^4.7.16"
  },
